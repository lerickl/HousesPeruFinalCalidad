
@using HousesPeru.Models;
@using HousesPeru.services;
@{
    ViewBag.Title = "GetInmueblesAdminOrUsuario";
    if (ViewBag.UserLogged == "ADMINISTRADOR")
    {
        Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
    }
    if (ViewBag.UserLogged == "USUARIO")
    {
        Layout = "~/Views/Shared/_LayoutUsuario.cshtml";
    }
    if (ViewBag.UserLogged == "AGENTE")
    {
        Layout = "~/Views/Shared/_LayoutAgente.cshtml";
    }
    if (ViewBag.UserLogged == null)
    {
        Layout = "~/Views/Shared/_Layout.cshtml";
    }
    var userxd = (Usuario)Session[AuthService.User.USUARIO];


}
<h1 id="ViewListInmuebkes"></h1>
<div class="er_ContPublicacion"></div>
<div class="er_cont_banner">
    <!-- Table with panel -->
    <div class="card card-cascade narrower" style="font-family: Bahnschrift;opacity:0.9">

        <!--Card image-->
        <div class="view view-cascade gradient-card-header blue-gradient narrower py-2 mx-4 mb-3 d-flex justify-content-between align-items-center">
 

            <a href="" class="white-text mx-3">Lista de Inmuebles: @userxd.Nombre</a>
 

        </div>
        <!--/Card image-->

        <div class="px-4">

            <div class="table-wrapper">
                <!--Table-->
                <table class="table table-responsive-md mb-0">

                    <!--Table head-->
                    <thead>
                        <tr class="table-primary">
                            <th>
                                @*<input class="form-check-input" type="checkbox" id="checkbox">
                                    <label class="form-check-label" for="checkbox" class="mr-2 label-table"></label>*@
                            </th>
                            @*<th class="th-lg">
                                    <a>
                                        Id
                                        <i class="fas fa-sort ml-1"></i>
                                    </a>
                                </th>*@
                            <th class="th-lg">
                                <a>
                                    Usuario
                                    <i class="fas fa-sort ml-1"></i>
                                </a>
                            </th>
                            <th class="th-lg">
                                <a href="">
                                    Inmueble
                                    <i class="fas fa-sort ml-1"></i>
                                </a>
                            </th>
                            <th class="th-lg">
                                <a href="">
                                    Tipo Inmueble
                                    <i class="fas fa-sort ml-1"></i>
                                </a>
                            </th>
                            <th class="th-lg">
                                <a href="">
                                    Fecha Publicacion
                                    <i class="fas fa-sort ml-1"></i>
                                </a>
                            </th>
                            <th class="th-lg">
                                <a href="">
                                    Fecha Fin Publicacion
                                    <i class="fas fa-sort ml-1"></i>
                                </a>
                            </th>
                            <th class="th-lg">
                                <a href="">
                                    Ciudad
                                    <i class="fas fa-sort ml-1"></i>
                                </a>
                            </th>
                            <th class="th-lg">
                                <a href="">
                                    Plan Usuario
                                    <i class="fas fa-sort ml-1"></i>
                                </a>
                            </th>

                        </tr>
                    </thead>
                    <!--Table head-->
                    <!--Table body-->
                    <tbody>
                        @{ var index = 1; }
                        @foreach (var inmueble in ViewBag.Inmuebles)
                        {
                            <tr>
                                <th scope="row">
                                    <a class="btn btn-outline-secondary btn-block  " href="/Inmueble/Inmueble?id=@inmueble.InmuebleId"> Editar <i class="fas fa-pen-square ml-1"></i></a>

                                </th>
                                @foreach (var user in ViewBag.Usuarios)
                                {
                                    if (inmueble.UsuarioId == user.UsuarioId)
                                    {

                                        <td>@user.Nombre</td>
                                        <td>@inmueble.NombreInm</td>

                                        foreach (var tipInm in ViewBag.TipoInmueble)
                                        {
                                            if (inmueble.InmuebleTipoId == tipInm.InmuebleTipoId)
                                            {
                                                <td>@tipInm.Nombre</td>
                                            }
                                        }
                                        <td>@user.FechaInicioPlan</td>
                                        <td>@user.FechaFinPlan</td>
                                        foreach (var ciudad in ViewBag.Ciudad)
                                        {
                                            if (inmueble.CiudadId == ciudad.CiudadesId)
                                            {
                                                <td>@ciudad.Nombre</td>
                                            }
                                        }
                                        foreach (var plan in ViewBag.Plan)
                                        {
                                            if (user.PlanId == plan.Id)
                                            {
                                                <td>@plan.Nombre</td>
                                            }
                                        }

                                    }


                                }

                            </tr>

                            index++;

                        }



                    </tbody>
                    <!--Table body-->
                </table>
                <!--Table-->
            </div>

        </div>

    </div>
    <!-- Table with panel -->
</div>


<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />

    <title>@ViewData["Title"] - HousesInPeru</title>
    <link href="https://fonts.googleapis.com/css?family=Roboto&display=swap" rel="stylesheet">
    <link href="~/Content/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/css/keyframes.css" rel="stylesheet" />
    <link href="~/Content/css/banner.css" rel="stylesheet" />
    <link href="~/Content/css/style.css" rel="stylesheet" />
    <link href="~/Content/css/botones.css" rel="stylesheet" />

    <link href="~/Content/css/carousel.css" rel="stylesheet" />
    <script src="~/Content/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/CustomJs/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/CustomJs/material.min.js"></script>
    <script src="~/CustomJs/sweetalert2.min.js"></script>
    @*<link href="~/Content/css/fontawesome.css" rel="stylesheet" />*@
    <link href="https://use.fontawesome.com/releases/v5.0.6/css/all.css" rel="stylesheet" />
    @*<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">*@


    <script src="https://cdn.jsdelivr.net/npm/vue"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/vue/2.2.0/vue.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/vue-resource/1.3.1/vue-resource.min.js"></script>


    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    @*<div include-cshtml="/Home/Login"></div>*@

</head>
<body>
    <hi id="LayoutInitial"> </hi>
    <header>
        <nav class="ewk_navbar navbar navbar-expand-lg navbar-light bg-light fixed-top" style=" font-family: Bahnschrift;">
            <a class="navbar-brand" href="/Home/Index"><img src="~/Content/images/logo.gif" />HOUSES IN PERÚ</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
                    aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item active">
                        <a class="nav-link" id="ButtnInicio" href="/Home/Index">Inicio <span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" id="ButtnAbout" href="/Home/About">Quienes Somos</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" id="ButtnContact" href="/Home/Contact">Contacto</a>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="" id="navbarDropdown" role="button" data-toggle="dropdown"
                           aria-haspopup="true" aria-expanded="false">
                            Servicios
                        </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                            <a class="dropdown-item" id="bttnServiciosVenta" href="/Inmueble/Buscar?Estado=Venta">Venta</a>
                            <a class="dropdown-item" id="bttnServiciosAlquiler" href="/Inmueble/Buscar?Estado=Alquiler">Alquiler</a>
                            <div class="dropdown-divider"></div>
                            <a class="dropdown-item" href="">Transacciones</a>
                        </div>
                    </li>


                </ul>
                <div>
                    <ul class="nav navbar-right">
                        <li class="nav navbar-nav mr-auto">
                            <a class="nav-link" id="ButtnPublicarInmueble" href="/Plan/GetPlanes"><span class="glyphicon glyphicon-home">Publicar Inmueble</span></a>
                            <a class="nav-link" id="ButtnRegistro" href="/Home/Registro"><span class="glyphicon glyphicon-home">Registrarse</span></a>
                            @*<a class="nav-link" href="" data-toggle="modal" data-target="#Registro"><span class="glyphicon glyphicon-user">Registrarse</span></a>*@
                            <a class="nav-link" id="ButtnLogin" href="" data-toggle="modal" data-target="#LogIn"><span class="glyphicon glyphicon-log-in">Ingresar</span></a>
                        </li>
                    </ul>
                </div>
            </div>

            <div class="modal fade" id="Modal-Charge" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="msgError"></div>
                    <div class="modal-content" id="ModalContent">

                    </div>
                </div>
            </div>
        </nav>


    </header>


    @RenderBody()

    <footer>
        <div class="card-group" style="    font-family: Bahnschrift;">
            <div class="card">

                <div class="card-body">
                    <h5 class="card-title">SIGUENOS EN</h5>
                    <hi></hi>
                    <div class="col-md-6">
                        <row class="er_sombra_Inmueble_containerShare">
                            <a name="share-fb" target="_blank" class="mx-1  " role="button" href="https://www.facebook.com/Agente-Inmobiliario-Alquila-yo-vende-104565081054886/"><i class="fab fa-facebook-f"></i></a>

                        </row>
                        <row class="er_sombra_Inmueble_containerSharewsp">
                            <a name="share-whatsapp" href="whatsapp://send?text=http://" data-action="share/whatsapp/share" class="mx-1 " role="button"><i class="fab fa-whatsapp"> </i></a>

                        </row>
                        <row class="er_sombra_Inmuble_ContainerShareInstagram">
                            <a name="share-Instagram" href="whatsapp://send?text=http://" data-action="share/whatsapp/share" class="mx-1 " role="button"><i class="fab fa-instagram"> </i></a>

                        </row>
                    </div>
                


                </div>
            </div>
            <div class="card">

                <div class="card-body">
                    <h5 class="card-title">NAVEGACIÓN</h5>
                    <p><a href="">Inicio</a></p>
                    <p><a href="">Compra</a></p>
                    <p><a href="">Alquiler</a></p>
                    <p><a href="">Acerca de Nosotros</a></p>
                    <p><a href="">Publicar Inmueble</a></p>
                    <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
                </div>
            </div>
            <div class="card">

                <div class="card-body ">
                    <h5 class="card-title">PROPIEDADES DESTACADAS</h5>
        
                    <div >
                        <div id="carouselpagd" class="carousel slide carousel-fade" data-ride="carousel">
                            <div id="main">

                                <ol class="carousel-indicators">
                                    <li class="active" :data-slide-to="0" data-target="#carouselpagd"></li>
                                    <li v-for="(item, index) in lists" v-if="index>0" class=" " :data-slide-to="index" data-target="#carouselpagd"></li>
                                </ol>
                                <div class="carousel-inner er_cont_carrousel_layout" id="Carrousel_Layout">

                                    <div class="carousel-item active" v-for="(item, index) in lists" v-if="index==0">
                           
                                        <a :href='"/Publicacion/GetInmuebleByID?IdInmueble="+item.InmuebleId' ><img class="d-block img-responsive  " loading="lazy" :src="item.Img"> </a>

                                    </div>

                                    <div class="carousel-item" v-for="(item, index) in lists" v-if="index>0">
                                        <a :href='"/Publicacion/GetInmuebleByID?IdInmueble="+item.InmuebleId'> <img class="d-block img-responsive  " loading="lazy" :src="item.Img"> </a>

                                    </div>
 
                                </div>

                                <a href="#carouselpagd" class="carousel-control-prev" role="button" data-slide="prev">
                                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                    <span class="sr-only">Previous</span>
                                </a>
                                <a href="#carouselpagd" class="carousel-control-next" role="button" data-slide="next">
                                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                    <span class="sr-only">Next</span>
                                </a>

                            </div>
                        </div>

                    </div>
                           
                     

                </div>
       
                    <p class="card-text"></p>
                    <p class="card-text"><small class="text-muted"></small></p>
                </div>
     
            <div class="card">

                <div class="card-body">
                    <h5 class="card-title">CONTACTO</h5>
                    <p class="card-text">Por ingresar</p>
                    <p class="card-text">Jr. no recuerdo lalala</p>
                    <p class="card-text">Tf: 076 - 123456</p>
                    <p class="card-text">Cel: 912345678.</p>
                    <p class="card-text">Cel: 912345678.</p>
                </div>
            </div>
        </div>
        <p>&copy; 2020 - HousesInPeru</p>
    </footer>
    <div class="modal fade" id="LogIn">
        <div class="modal-dialog form-dark" role="document">

            <div class="modal-content card card-image img-fluid" style="background-image: url('https://www.gratistodo.com/wp-content/uploads/2017/02/Casas-Modernas-7.jpg');">
                <div class="text-white rgba-stylish-strong py-5 px-5 z-depth-4">

                    <div class="modal-header text-center pb-4">
                        <h3 class="modal-title w-100 white-text font-weight-bold" id="myModalLabel">
                            <strong>INGRESAR</strong> <a class="green-text font-weight-bold"><strong></strong></a>
                        </h3>
                        <button type="button" class="close white-text" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>

                    <div class="modal-body">
                       
                        <form method="post" action="/Home/Login" class="form1">
                            @Html.ValidationMessage("Cuenta")
                            <div class="md-form mb-5">
                                <label style="color:cornflowerblue" data-error="wrong" data-success="right" for="Form-email5">Tu correo electronico</label>
                                <input type="email" id="Email" class="form-control validate white-text col-md-12" ¨placeholder=" " name="Email">

                            </div>
                            <div class="md-form pb-3">
                                <label style="color:cornflowerblue" data-error="wrong" data-success="right" for="Form-pass5">Tu contraseña</label>
                                <input type="password" id="Password" class="form-control validate white-text col-md-12" ¨placeholder="" name="Password">

                            </div>
                            <div class="row d-flex align-items-center mb-4 ">
                                <div class="text-center mb-3 col-md-12">
                                    <button id="BottonIngresarLogin" type="submit" class="btn btn-success btn-block btn-rounded z-depth-1">Ingresar</button>
                                </div>
                            </div>
                            <h1 id="modalLogin"></h1>
                        </form>
                        <div class="row">


                            <div class="col-md-12">
                                <p class="font-small white-text d-flex justify-content-end">
                                    ¿Nuevo en HousesInPeru? <a href="/Home/Registro" class="green-text ml-1 font-weight-bold">
                                        Registrarse
                                    </a>
                                </p>
                            </div>
                        </div>


                    </div>
                </div>
            </div>


        </div>

    </div>
    @*@{ ViewBag.ShowModalLogin = false;}
        @if (ViewBag.ShowModalLogin)
        {
            <script> $("#LogIn").modal("show");</script>

        }*@
    <script>
      
        </script>
    <script src="~/CustomJs/Main.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("Scripts", required: false)
</body>
</html>
 
